            <div class="row">

                <div class="col-md-6 widget">
                    <div class="widget-body">
                        <p class="simplenav">
                            <a href="{{ path('home_page') }}">На главную страницу</a> |
                            <a href="{{ path('about') }}">О нас</a> |
                            <a href="{{ path('contact') }}">Контакты</a> |
