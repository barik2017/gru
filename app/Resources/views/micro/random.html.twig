                        <li><a href="{{ path('all') }}">Машина + грузчкики</a></li>

                    </ul>
                </li>
                <li><a href="{{ path('contact') }}">Наши контакты</a></li>
                <li><a href="{{ path('reviews') }}">Отзывы</a> </li>
        <!--   <li class="text-success"> Our number <br> 09955588899 <br> 5559994465</li> -->
            </ul>
        </div><!--/.nav-collapse -->
