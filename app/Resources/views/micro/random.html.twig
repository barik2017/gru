            <a class="navbar-brand" href="{{ path('home_page') }}"><img src= {{ asset('images/logo.png') }} ></a>
        </div>
        <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav pull-right">
                <li class="active"><a href="{{ path('home_page') }}">Главная страница</a></li>
                <li><a href="{{ path('about') }}">О нас</a></li>
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Наши услуги <b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li class="active"><a href="{{ path('trucking') }}">Грузоперевозки</a></li>
